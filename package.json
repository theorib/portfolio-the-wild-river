{
  "name": "testing-next-js",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write \"**/*.{ts,tsx,md,json,js,jsx}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md,json,js,jsx}\"",
    "lint": "eslint . --quiet && tsc --noEmit --skipLibCheck",
    "lint:staged": "pnpm lint-staged --config lint-staged.config.ts",
    "lint:fix": "eslint . --cache --fix",
    "lint:next": "next lint",
    "lint:debug": "eslint . --debug",
    "lint:debug:prettier": "eslint-config-prettier src/app/layout.tsx",
    "lint:debug:timing": "TIMING=1 eslint",
    "lint:inspect": "pnpm dlx @eslint/config-inspector@latest",
    "lint:tsc": "tsc --noEmit --skipLibCheck",
    "test": "vitest watch --browser.headless",
    "test:ui": "vitest watch --ui",
    "test:coverage": "vitest run --browser.headless --coverage",
    "test:e2e": "exec playwright test",
    "test:e2e:ui": "exec playwright test --ui",
    "test:e2e:report": "exec playwright show-report",
    "test:e2e:debug": "exec playwright test --debug",
    "test:e2e:codegen": "exec playwright codegen",
    "prepare": "husky",
    "shad": "pnpm dlx shadcn@canary add",
    "db:push": "pnpm drizzle-kit push --config drizzle.main.config.ts",
    "db:start:local": "turso dev --db-file ./tursoLocal/turso_local.db --port 8088",
    "db:start:studio": "pnpm drizzle-kit studio --verbose --config drizzle.main.config.ts --host 127.0.0.1 --port 8087",
    "db:generate": "pnpm drizzle-kit generate --config drizzle.main.config.ts",
    "db:migrate": "tsx ./src/db/scripts/migrate.ts",
    "db:seed": "tsx ./src/db/scripts/seed.ts",
    "db:clear": "tsx ./src/db/scripts/clearDb.ts",
    "db:clear:records": "tsx ./src/db/scripts/clearRecords.ts",
    "cz": "bash ./enhanced_commit.sh"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@lucia-auth/adapter-drizzle": "^1.1.0",
    "@node-rs/argon2": "^2.0.2",
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@tabler/icons-react": "^3.29.0",
    "@tanstack/react-query": "^5.66.0",
    "@tanstack/react-query-devtools": "^5.66.0",
    "@zxcvbn-ts/core": "^3.0.4",
    "@zxcvbn-ts/language-common": "^3.0.4",
    "@zxcvbn-ts/language-en": "^3.0.2",
    "axios": "^1.7.9",
    "babel-plugin-react-compiler": "19.0.0-beta-30d8a17-20250209",
    "better-sqlite3": "^11.8.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookies-next": "^5.1.0",
    "dotenv": "^16.4.7",
    "drizzle-kit": "^0.30.4",
    "drizzle-orm": "^0.39.3",
    "drizzle-zod": "^0.7.0",
    "install": "^0.13.0",
    "lucide-react": "^0.475.0",
    "next": "15.1.7",
    "next-themes": "^0.4.4",
    "oslo": "1.2.1",
    "rate-limiter-flexible": "^5.0.5",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.54.2",
    "react-icons": "^5.4.0",
    "server-only": "^0.0.1",
    "sonner": "^1.7.2",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "typeid-js": "^1.2.0",
    "use-debounce": "^10.0.4",
    "zod": "^3.24.1",
    "zod-validation-error": "^3.4.0",
    "zsa": "^0.6.0",
    "zsa-react": "^0.2.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@commitlint/cz-commitlint": "^19.6.1",
    "@eslint/compat": "^1.2.6",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.20.0",
    "@next/eslint-plugin-next": "^15.1.7",
    "@playwright/test": "^1.50.1",
    "@tailwindcss/postcss": "^4.0.6",
