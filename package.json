{
  "name": "testing-next-js",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "cz": "bash ./enhanced_commit.sh",
    "dev": "next dev",
    "format": "prettier --write \"**/*.{ts,tsx,md,json,js,jsx}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md,json,js,jsx}\"",
    "preinstall": "npx only-allow pnpm",
    "lint": "eslint . --quiet && tsc --noEmit --skipLibCheck",
    "lint:debug": "eslint . --debug",
    "lint:debug:prettier": "eslint-config-prettier src/app/layout.tsx",
    "lint:debug:timing": "TIMING=1 eslint",
    "lint:fix": "eslint . --cache --fix",
    "lint:inspect": "pnpm dlx @eslint/config-inspector@latest",
    "lint:next": "next lint",
    "lint:staged": "pnpm lint-staged -v --config lint-staged.config.mjs",
    "lint:tsc": "tsc --noEmit --skipLibCheck",
    "package:sort": "pnpm dlx sort-package-json",
    "prepare": "husky",
    "shad": "pnpm dlx shadcn@canary add",
    "start": "next start",
    "supabase:generate:schemas": "supazod -i src/lib/types/supabase.types.ts -o src/lib/schemas/supabaseSchemas.ts && eslint --fix src/lib/schemas/supabaseSchemas.ts",
    "supabase:generate:types": "dotenv -- supabase gen types typescript --project-id=\"$SUPABASE_PROJECT_ID\" > src/lib/types/supabase.types.ts && eslint --fix src/lib/types/supabase.auto.types.ts",
    "test": "vitest watch --browser.headless",
    "test:coverage": "vitest run --browser.headless --coverage",
    "test:e2e": "exec playwright test",
    "test:e2e:codegen": "exec playwright codegen",
    "test:e2e:debug": "exec playwright test --debug",
    "test:e2e:report": "exec playwright show-report",
    "test:e2e:ui": "exec playwright test --ui",
    "test:ui": "vitest watch --ui"
  },
